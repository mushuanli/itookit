{
  "name": "@itookit/vfs-ui",
  "version": "0.4.1",
  "description": "A modular UI library for managing Virtual File System interfaces with components for file/folder navigation, tagging, and content outline",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./style.css": "./src/styles/index.css"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/vfs-ui.umd.cjs",
    "module": "./dist/vfs-ui.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/vfs-ui.js",
        "require": "./dist/vfs-ui.umd.cjs"
      },
      "./style.css": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "vfs",
    "vfs-ui",
    "vfs-core",
    "virtual-file-system",
    "ui-components",
    "tree-view",
    "editor-adapter",
    "content-management",
    "file-browser",
    "outline-view",
    "browser-ui",
    "typescript"
  ],
  "author": "lizlok@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mushuanli/itookit.git",
    "directory": "packages/vfs-ui"
  },
  "bugs": {
    "url": "https://github.com/mushuanli/itookit/issues"
  },
  "homepage": "https://github.com/mushuanli/itookit/tree/main/packages/vfs-ui#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@itookit/common": "workspace:*",
    "immer": "^10.2.0"
  },
  "peerDependencies": {
    "@itookit/vfs-core": "workspace:*"
  },
  "devDependencies": {
    "@itookit/vfs-core": "workspace:*",
    "@types/node": "^20.19.25",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.1"
  }
}
