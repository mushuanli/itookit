{
  "name": "@itookit/vfs-ui",
  "version": "1.0.0",
  "description": "A UI component library for visualizing and managing file hierarchies from @itookit/vfs-core.",
  "type": "module",
  
  "main": "./dist/vfs-ui.umd.js",
  "module": "./dist/vfs-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vfs-ui.es.js",
      "require": "./dist/vfs-ui.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "keywords": [
    "vfs",
    "vfs-ui",
    "vfs-core",
    "virtual-file-system",
    "ui-components",
    "tree-view",
    "editor-adapter",
    "content-management",
    "file-browser",
    "outline-view",
    "browser-ui",
    "typescript"
  ],
  "author": "lizlok@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mushuanli/itookit.git",
    "directory": "packages/vfs-ui"
  },
  "bugs": {
    "url": "https://github.com/mushuanli/itookit/issues"
  },
  "homepage": "https://github.com/mushuanli/itookit/tree/main/packages/vfs-ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "@itookit/vfs-core": "^0.0.1"
  },
  "dependencies": {
    "@itookit/common": "workspace:*",
    "immer": "^10.0.0"
  },
  "devDependencies": {
    "@itookit/vfs-core": "workspace:*",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "typescript": "^5.3.0",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.0.0"
  }
}
