{
  "name": "@itookit/llm-driver",
  "version": "0.1.0",
  "description": "A unified, TypeScript-based LLM interface driver supporting OpenAI, Anthropic, Gemini and more.",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "verify": "tsx scripts/verify.ts"
  },
  "keywords": [
    "itookit",
    "llm",
    "driver",
    "openai",
    "anthropic",
    "gemini",
    "ai"
  ],
  "author": "lizlok@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mushuanli/itookit.git",
    "directory": "packages/llm-driver"
  },
  "bugs": {
    "url": "https://github.com/mushuanli/itookit/issues"
  },
  "homepage": "https://github.com/mushuanli/itookit/tree/main/packages/llm-driver#readme",

  "dependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/vfs-core": "workspace:*",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.25",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
