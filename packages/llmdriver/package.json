{
  "name": "@itookit/llmdriver",
  "version": "0.1.0",
  "description": "A unified, TypeScript-based LLM interface driver supporting OpenAI, Anthropic, Gemini and more.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "itookit",
    "llm",
    "driver",
    "openai",
    "anthropic",
    "gemini",
    "ai"
  ],
  "author": "lizlok@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mushuanli/itookit.git",
    "directory": "packages/llmdriver"
  },
  "bugs": {
    "url": "https://github.com/mushuanli/itookit/issues"
  },
  "homepage": "https://github.com/mushuanli/itookit/tree/main/packages/llmdriver#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@itookit/common": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
