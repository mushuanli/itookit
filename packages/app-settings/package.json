{
  "name": "@itookit/app-settings",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./style.css": "./src/styles/styles.css"
  },
  "publishConfig": {
    "main": "./dist/app-settings.umd.cjs",
    "module": "./dist/app-settings.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/app-settings.js",
        "require": "./dist/app-settings.umd.cjs"
      },
      "./style.css": "./dist/style.css"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/vfs": "workspace:*",
    "@itookit/llm-driver": "workspace:*",
    "@itookit/llm-engine": "workspace:*",
    "@itookit/llm-ui": "workspace:*"
  },
  "devDependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/vfs": "workspace:*",
    "@itookit/llm-driver": "workspace:*",
    "@itookit/llm-engine": "workspace:*",
    "@itookit/llm-ui": "workspace:*",
    "@types/node": "^20.19.25",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1"
  }
}
