{
  "name": "@itookit/llm-engine",
  "version": "0.1.0",
  "description": "The headless orchestration core for LLM workspaces. Handles session state management, LLM execution, VFS persistence, and agent logic without UI dependencies.",
  "type": "module",
  "main": "./dist/llm-engine.umd.cjs",
  "module": "./dist/llm-engine.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/llm-engine.js",
      "require": "./dist/llm-engine.umd.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "llm-engine",
    "headless",
    "agent-orchestrator",
    "session-management",
    "vfs",
    "llm-driver",
    "typescript"
  ],
  "author": "lizlok@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mushuanli/itookit.git",
    "directory": "packages/llm-engine"
  },
  "bugs": {
    "url": "https://github.com/mushuanli/itookit/issues"
  },
  "homepage": "https://github.com/mushuanli/itookit/tree/main/packages/llm-engine#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/vfs-core": "workspace:*",
    "@itookit/llmdriver": "workspace:*"
  },
  "dependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/llmdriver": "workspace:*",
    "@itookit/vfs-core": "workspace:*",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.25",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.1"
  }
}
