{
  "name": "@itookit/llm-engine",
  "version": "0.1.0",
  "description": "LLM Session Engine - UI Adapter Layer",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "llm-engine",
    "headless",
    "agent-orchestrator",
    "session-management",
    "vfs",
    "llm-driver",
    "typescript"
  ],
  "author": "lizlok@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mushuanli/itookit.git",
    "directory": "packages/llm-engine"
  },
  "bugs": {
    "url": "https://github.com/mushuanli/itookit/issues"
  },
  "homepage": "https://github.com/mushuanli/itookit/tree/main/packages/llm-engine#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/vfs-core": "workspace:*",
    "@itookit/llm-kernel": "workspace:*",
    "@itookit/llm-driver": "workspace:*"
  },
  "dependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/llm-driver": "workspace:*",
    "@itookit/llm-kernel": "workspace:*",
    "@itookit/vfs-core": "workspace:*"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.25",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.1"
  }
}
