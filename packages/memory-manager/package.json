{
    "name": "@itookit/memory-manager",
    "version": "0.4.1",
    "description": "An intelligent glue layer connecting VFS-UI and MDxEditor with headless AI capabilities for advanced memory management.",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./style.css": "./src/styles/memory-manager.css"
  },
  "publishConfig": {
        "access": "public",
    "main": "./dist/memory-manager.umd.cjs",
    "module": "./dist/memory-manager.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/memory-manager.js",
        "require": "./dist/memory-manager.umd.cjs"
      },
      "./style.css": "./dist/style.css"
    }
  },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
        "preview": "vite preview",
    "prepublishOnly": "pnpm build"
  },
    "keywords": [
        "memory-manager",
        "pkm",
        "vfs-ui",
        "mdxeditor",
        "headless-ai",
        "typescript"
    ],
    "author": "lizlok@gmail.com",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/mushuanli/itookit.git",
        "directory": "packages/memory-manager"
    },
    "engines": {
        "node": ">=16.0.0"
    },
  "dependencies": {
    "immer": "^10.2.0"
  },
  "peerDependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/mdxeditor": "workspace:*",
    "@itookit/vfs-core": "workspace:*",
    "@itookit/vfs-ui": "workspace:*"
  },
  "devDependencies": {
    "@itookit/common": "workspace:*",
    "@itookit/mdxeditor": "workspace:*",
    "@itookit/vfs-core": "workspace:*",
    "@itookit/vfs-ui": "workspace:*",
    "@types/node": "^20.19.25",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1"
  }
}
