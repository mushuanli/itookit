{
  "name": "@itookit/common",
  "version": "0.4.1",
  "description": "Common utilities, types, and components for the itookit ecosystem.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/index.css",
    "./index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts && pnpm copy-assets",
    "copy-assets": "cpx \"src/**/*.css\" dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "itookit",
    "common"
  ],
  "author": "lizlok@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mushuanli/itookit.git",
    "directory": "packages/common"
  },
  "bugs": {
    "url": "https://github.com/mushuanli/itookit/issues"
  },
  "homepage": "https://github.com/mushuanli/itookit/tree/main/packages/common#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
